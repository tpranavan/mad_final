<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/home"
    tools:context=".AD_vehicle">


        <TextView
            android:id="@+id/textView5"
            android:layout_width="151dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="24dp"
            android:text="Vehicle Type"
            android:textColor="@android:color/background_dark"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView10" />

        <EditText
            android:id="@+id/type"
            android:layout_width="140dp"
            android:layout_height="50dp"
            android:layout_marginStart="27dp"
            android:layout_marginLeft="27dp"
            android:layout_marginTop="25dp"
            android:ems="10"
            android:hint="Enter the Vehicle_Type"
            android:inputType="textPersonName"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/id" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="151dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="28dp"
            android:text="No of Vehicle"
            android:textColor="@android:color/background_dark"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <EditText
            android:id="@+id/vehicle"
            android:layout_width="215dp"
            android:layout_height="50dp"
            android:layout_marginStart="27dp"
            android:layout_marginLeft="27dp"
            android:layout_marginTop="25dp"
            android:ems="10"
            android:hint="Enetr the No_of_Vehicles"
            android:inputType="number"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            app:layout_constraintTop_toBottomOf="@+id/type" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="151dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="28dp"
            android:text="Vehicle Cost"
            android:textColor="@android:color/background_dark"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <EditText
            android:id="@+id/cost"
            android:layout_width="218dp"
            android:layout_height="45dp"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="24dp"
            android:ems="10"
            android:hint="Enter the Vehicle_Cost"
            android:inputType="number"
            app:layout_constraintStart_toEndOf="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/vehicle" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="155dp"
            android:layout_height="63dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="32dp"
            android:text="Vehicle Facilities"
            android:textColor="@android:color/background_dark"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />

        <EditText
            android:id="@+id/fac"
            android:layout_width="215dp"
            android:layout_height="50dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="36dp"
            android:ems="10"
            android:hint="Enter the Vehicle_Facilities"
            android:inputType="textPersonName"
            app:layout_constraintStart_toEndOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/cost" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="149dp"
            android:layout_height="53dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:text="Vehicle ID"
            android:textColor="@android:color/background_dark"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="184dp" />

        <EditText
            android:id="@+id/id"
            android:layout_width="207dp"
            android:layout_height="53dp"
            android:layout_marginStart="28dp"
            android:layout_marginLeft="28dp"
            android:ems="10"
            android:hint="Vehicle_ID"
            android:inputType="textPersonName"
            app:layout_constraintStart_toEndOf="@+id/textView10"
            tools:layout_editor_absoluteY="184dp" />

        <Button
            android:id="@+id/fb1"
            android:layout_width="79dp"
            android:layout_height="35dp"
            android:layout_marginStart="176dp"
            android:layout_marginLeft="176dp"
            android:layout_marginTop="20dp"
            android:background="@color/colorPrimaryDark"
            android:text="ADD"
            android:textColor="@android:color/background_light"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/fac" />

        <Button
            android:id="@+id/search"
            android:layout_width="82dp"
            android:layout_height="36dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="12dp"
            android:background="@color/colorPrimaryDark"
            android:text="SEARCH"
            android:textColor="@android:color/background_light"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/button7"
            app:layout_constraintTop_toBottomOf="@+id/fb1" />

        <Button
            android:id="@+id/button7"
            android:layout_width="88dp"
            android:layout_height="39dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="44dp"
            android:background="@color/navbar2"
            android:text="View All"
            android:textColor="@android:color/background_light"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView8" />

        <Button
            android:id="@+id/button4"
            android:layout_width="88dp"
            android:layout_height="36dp"
            android:layout_marginStart="12dp"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="12dp"
            android:background="@color/navbar2"
            android:text="Update"
            android:textColor="@android:color/background_light"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/search"
            app:layout_constraintTop_toBottomOf="@+id/fb1" />

        <Button
            android:id="@+id/fb2"
            android:layout_width="87dp"
            android:layout_height="36dp"
            android:layout_marginStart="12dp"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="64dp"
            android:background="@color/navbar2"
            android:text="Delete"
            android:textColor="@android:color/background_light"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/button4"
            app:layout_constraintTop_toBottomOf="@+id/fac" />

        <Button
            android:id="@+id/log"
            android:layout_width="70dp"
            android:layout_height="32dp"
            android:background="@color/navbar2"
            android:text="LOGOUT"
            android:textColor="@android:color/background_light"
            tools:layout_editor_absoluteX="328dp"
            tools:layout_editor_absoluteY="16dp" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="186dp"
            android:layout_height="38dp"
            android:layout_marginStart="112dp"
            android:layout_marginLeft="112dp"
            android:text="Vehicle Details"
            android:textColor="@android:color/background_dark"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="121dp" />

        <Spinner
            android:id="@+id/spinner1"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:layout_marginStart="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="25dp"
            app:layout_constraintStart_toEndOf="@+id/type"
            app:layout_constraintTop_toBottomOf="@+id/id" />

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="332dp"
            android:layout_height="111dp"
            app:srcCompat="@drawable/ic_sdcar"
            tools:layout_editor_absoluteX="28dp"
            tools:layout_editor_absoluteY="10dp" />


</androidx.constraintlayout.widget.ConstraintLayout>